{
  "kind": "collectionType",
  "collectionName": "portfolio",
  "info": {
    "singularName": "project",
    "pluralName": "portfolio",
    "displayName": "portfolio",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "project_name": {
      "type": "string",
      "required": true
    },
    "project_type": {
      "type": "enumeration",
      "enum": [
        "Personal Project",
        "Client Project"
      ]
    },
    "project_cover": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "project_data": {
      "type": "date",
      "required": true
    },
    "project_live_url": {
      "type": "string",
      "required": true
    },
    "featured": {
      "type": "boolean",
      "required": true
    },
    "technologies": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::technology.technology"
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "overvier": {
      "type": "string",
      "required": true
    },
    "project_github_link": {
      "type": "string",
      "required": true
    },
    "client_name": {
      "type": "string",
      "required": true
    },
    "avatar": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "project_name",
      "required": true
    }
  }
}
